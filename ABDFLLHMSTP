-- [[ MEGA HYBRID SYSTEM 2026 - GHOST VM x ABADIFM ]]
-- Version: 2.0.26 | Full Integration

local _PAYLOAD = ""

-- SERVICES
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local StarterGui = game:GetService("StarterGui")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

-- [[ 1. VM DECODER & EXECUTION ]]
local function _VM_EXECUTE(stream)
    local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
    local data = stream:gsub('[^'..b..'=]', '')
    local decoded = (data:gsub('.', function(x)
        if (x == '=') then return '' end
        local r,f='',(b:find(x)-1)
        for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end
        return r;
    end):gsub('%d%d%d%d%d%d%d%d', function(x)
        local c=0
        for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end
        return string.char(c)
    end))
    loadstring(decoded)()
end

-- [[ 2. CONFIGURATION ]]
getgenv().Config = {
    FlySpeed = 100, WalkSpeed = 100, JumpPower = 150, HitboxSize = 25,
    RingRadius = 20, RingActive = true, BlackholeActive = false,
    Noclip = false, Flying = false, Aimlock = false
}

local bodyVelocity, bodyGyro
local RingParts = {}

-- [[ 3. NOTIFICATION SYSTEM ]]
local function GetAIFeeling()
    local hour = os.date("*t").hour
    if hour >= 0 and hour <= 5 then return "Kegelapan abadi dimulai."
    elseif hour >= 6 and hour <= 11 then return "Lihat mereka gemetar."
    else return "Malam ini, biarkan keputusasaan merajalela." end
end

StarterGui:SetCore("SendNotification", {
    Title = "HYBRID VM LOADED",
    Text = "Owner: ABADIFM\n" .. GetAIFeeling(),
    Duration = 8
})

-- [[ 4. HITBOX ENGINE ]]
local function applyHitbox(player)
    if player == LocalPlayer then return end
    local function setup(char)
        local hrp = char:WaitForChild("HumanoidRootPart", 5)
        if hrp then
            task.wait(0.5)
            hrp.Size = Vector3.new(getgenv().Config.HitboxSize, getgenv().Config.HitboxSize, getgenv().Config.HitboxSize)
            hrp.Transparency = 0.7
            hrp.CanCollide = false
        end
    end
    if player.Character then setup(player.Character) end
    player.CharacterAdded:Connect(setup)
end
for _, p in pairs(Players:GetPlayers()) do applyHitbox(p) end
Players.PlayerAdded:Connect(applyHitbox)

-- [[ 5. MOVEMENT & PHYSICS ]]
RunService.Stepped:Connect(function()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        local hum = LocalPlayer.Character.Humanoid
        hum.WalkSpeed = getgenv().Config.WalkSpeed
        hum.JumpPower = getgenv().Config.JumpPower
        if getgenv().Config.Noclip then
            for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
                if v:IsA("BasePart") then v.CanCollide = false end
            end
        end
    end
end)

-- [[ 6. BLACKHOLE LOGIC ]]
local function managePart(part)
    if part:IsA("BasePart") and not part.Anchored and not part:IsDescendantOf(LocalPlayer.Character) then
        table.insert(RingParts, part)
    end
end
workspace.DescendantAdded:Connect(managePart)
for _, v in pairs(workspace:GetDescendants()) do managePart(v) end

RunService.Heartbeat:Connect(function()
    local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not root or not (getgenv().Config.RingActive or getgenv().Config.BlackholeActive) then return end
    for i, part in ipairs(RingParts) do
        if part.Parent and not part.Anchored then
            local angle = tick() * 10 + i
            local radius = getgenv().Config.BlackholeActive and 5 or getgenv().Config.RingRadius
            local targetPos = root.Position + Vector3.new(math.cos(angle) * radius, 2, math.sin(angle) * radius)
            part.Velocity = (targetPos - part.Position) * 15
        else table.remove(RingParts, i) end
    end
end)

-- [[ 7. FLY & KEYBINDS ]]
local function toggleFly()
    getgenv().Config.Flying = not getgenv().Config.Flying
    local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not root then return end
    if getgenv().Config.Flying then
        LocalPlayer.Character.Humanoid.PlatformStand = true
        bodyVelocity = Instance.new("BodyVelocity", root)
        bodyVelocity.MaxForce = Vector3.new(1,1,1) * math.huge
        bodyGyro = Instance.new("BodyGyro", root)
        bodyGyro.MaxTorque = Vector3.new(1,1,1) * math.huge
    else
        LocalPlayer.Character.Humanoid.PlatformStand = false
        if bodyVelocity then bodyVelocity:Destroy() end
        if bodyGyro then bodyGyro:Destroy() end
    end
end

UIS.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.F then toggleFly()
    elseif input.KeyCode == Enum.KeyCode.G then getgenv().Config.Noclip = not getgenv().Config.Noclip
    elseif input.KeyCode == Enum.KeyCode.B then getgenv().Config.BlackholeActive = not getgenv().Config.BlackholeActive
    end
end)

RunService.RenderStepped:Connect(function()
    if getgenv().Config.Flying and bodyVelocity and bodyGyro then
        local moveDir = Vector3.new(0,0,0)
        local cam = workspace.CurrentCamera
        if UIS:IsKeyDown("W") then moveDir += cam.CFrame.LookVector end
        if UIS:IsKeyDown("S") then moveDir -= cam.CFrame.LookVector end
        if UIS:IsKeyDown("A") then moveDir -= cam.CFrame.RightVector end
        if UIS:IsKeyDown("D") then moveDir += cam.CFrame.RightVector end
        bodyVelocity.Velocity = moveDir * getgenv().Config.FlySpeed
        bodyGyro.CFrame = cam.CFrame
    end
end)

-- [[ 8. FINAL EXECUTION ]]
_VM_EXECUTE(_PAYLOAD)
print("MEGA HYBRID SYSTEM LOADED - 2026")
