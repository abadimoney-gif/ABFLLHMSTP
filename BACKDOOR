--[[ 
    GHOST VM - 2026 HYBRID EDITION 
    Combined with Encrypted Loader V2
--]]

local _0x7f2a = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"

-- Virtual Machine Logic
local function _VM_EXECUTE(_BYTECODE_STREAM)
    local _DEC = function(data)
        local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
        data = string.gsub(data, '[^'..b..'=]', '')
        return (data:gsub('.', function(x)
            if (x == '=') then return '' end
            local r,f='',(b:find(x)-1)
            for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end
            return r;
        end):gsub('%d%d%d%d%d%d%d%d', function(x)
            local c=0
            for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end
            return string.char(c)
        end))
    end
    
    if not game:IsLoaded() then game.Loaded:Wait() end
    
    local _CORE_RAW = _DEC(_BYTECODE_STREAM)
    local success, err = pcall(function()
        return loadstring(_CORE_RAW)()
    end)
    
    if not success then warn("VM Error: " .. err) end
end

-- PAYLOAD: Ini adalah Ghost VM Core + Payload Tambahan yang sudah di-encode ke Base64
-- Fitur: Stiff Mode, Anti-Detect, Teleport, Infinite Yield, & Remote Scanner
local _PAYLOAD = ""

-- Execute combined system
_VM_EXECUTE(_PAYLOAD)
